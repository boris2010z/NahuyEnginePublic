{
    float4 position : SV_Position;
    float3 norm : NORMAL;
    float3 tangent : TANGENT;
    float2 texCoord : TEXCOORD;
    float3 p2e : TEXCOORD1;
};

struct VsOutputZPrepass
{
    float4 position : SV_Position;
    nointerpolation uint4 uid : TEXCOORD0;
};

struct VsOutputResolve
{
    float4 position : SV_Position;
    float2 texCoord     : TEXCOORD0;
};
