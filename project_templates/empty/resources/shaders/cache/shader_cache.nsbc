#define CONCAT(prefix, name) CONCAT_(prefix, name)

#define GLOBAL_CBUFFER(name) cbuffer CONCAT(GLOBAL_BUFFER_PREFIX, name)
#define SYSTEM_CBUFFER(name) cbuffer CONCAT(SYSTEM_BUFFER_PREFIX, name)

